 
 
 
<div class="row">
<div class="col-sm-6">
  <h1>Users</h1>
</div>

<div class="col-sm-6 text-right">

</div>
</div>

<div class="table-responsive">
<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th> Picture </th>
      <th>Name </th>
      <th> Email </th>
      <th> status </th>
      

          
        <th></th>
      
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <%= content_tag :tr, id: dom_id(user), class: dom_class(user) do %>
        
        
        <% if user.first_name.present? && user.last_name.presnt? %>
          <td><%=first_name%> <%=last_name %></td>
        <% else %>
          <td> N/A </td>
        <% end %><td>
                  <td><%= (user.email) %></td>
        
        
          <td><%= link_to 'Show', user %></td>
        
      <% end %>
    <% end %>
  </tbody>
</table>
</div>